def login(email, password):
    if not isinstance(email, str):
        return "email must be a string"
    if "@" not in email:
        return "add a @"
    if not isinstance(password, str):
        return "password must be a string"
    if len(password) < 8:
        return "make your password over 8 characters"
    if not any(int.isdigit()for int in password):
        return "add a number"
    if not any (up.isupper()for up in password):
        return "make a letter uppercase"
    return{"email": email, "password": password}
print(login("em@il", "Password5"))
